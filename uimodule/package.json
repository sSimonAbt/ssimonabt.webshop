{
    "name": "uimodule",
    "version": "0.0.1",
    "private": true,
    "description": "uimodule description",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "devDependencies": {
        "@openui5/types": "^1.138.0",
        "@sap-ux/eslint-plugin-fiori-tools": "^0.4.0",
        "@sap/ux-specification": "UI5-1.136",
        "@sap/ux-ui5-tooling": "1",
        "@sapui5/types": "~1.136.0",
        "@typescript-eslint/eslint-plugin": "^7.1.1",
        "@typescript-eslint/parser": "^7.1.1",
        "@ui5/cli": "^3.0.0",
        "@ui5/linter": "latest",
        "@wdio/cli": "^8.45.0",
        "@wdio/local-runner": "^8.45.0",
        "@wdio/mocha-framework": "^8.45.0",
        "@wdio/spec-reporter": "^8.43.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "ui5-task-zipper": "^3.2.1",
        "ui5-tooling-transpile": "^3.3.7",
        "wdio-ui5-service": "^2.2.1"
    },
    "scripts": {
        "start": "ui5 serve --config=ui5.yaml --open index.html",
        "build": "ui5 build self-contained --all --include-task generateVersionInfo --config=ui5.yaml --clean-dest --dest dist",
        "lint": "eslint ./",
        "ts-typecheck": "tsc --noEmit",
        "prestart": "npm run ts-typecheck",
        "prebuild": "npm run ts-typecheck",
        "ui5lint": "ui5lint",
        "qunit": "fiori run --open test/unitTests.qunit.html",
        "wdi5": "wdio run ./webapp/test/e2e/wdio.conf.ts"
    },
    "sapux": true
}
